#: ../resources/shutdown/js/utils.coffee:24
msgid "Lock"
msgstr "Κλείδωμα"

#: ../resources/shutdown/js/utils.coffee:24
msgid "Switch user"
msgstr "Εναλλαγή χρήστη"

#: ../resources/shutdown/js/utils.coffee:24
msgid "Log out"
msgstr "Αποσύνδεση"
